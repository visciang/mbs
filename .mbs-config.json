{
    "project": "mbs",
    "log": {
        "level": "info",
        "color": true
    },
    "remote_cache": {
        "push": false,
        "volume": null,
        "docker_registry": null
    },
    "timeout": 900,
    "files_profile": {
        "elixir": [
            "!{deps,_build,.elixir_ls}",
            ".formatter.exs",
            "mix.{exs,lock}",
            "config/*.exs",
            "{apps,lib,test}/**/*.{ex,exs}"
        ],
        "elixir-cache": [
            "!{deps,_build,.elixir_ls}",
            "mix.{exs,lock}"
        ]
    }
}