{
    "project": "mbs",
    "log": {
        "level": "info",
        "color": true
    },
    "volume": {
        "releases": "mbs-mbs-releases",
        "graph": "$BASEDIR/.mbs-graph"
    },
    "cache": {
        "local": {
            "volume": "mbs-mbs-local-cache"
        },
        "remote": {
            "push": false,
            "volume": "",
            "docker_registry": ""
        }
    },
    "timeout": 600,
    "files_profile": {
        "elixir": [
            ".formatter.exs",
            "mix.{exs,lock}",
            "config/*.exs",
            "{apps,lib,test}/**/*.{ex,exs}"
        ]
    }
}